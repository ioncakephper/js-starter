{
  "name": "js-starter",
  "version": "1.0.0",
  "description": "A robust starter template for modern Node.js development, pre-configured with Jest for testing, ESLint for linting, and Prettier for code formatting.",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --coverage --passWithNoTests",
    "test:watch": "jest --watchAll",
    "lint": "eslint \"**/*.{js,md}\"",
    "lint:fix": "eslint \"**/*.{js,md}\" --fix",
    "format": "prettier --write \"**/*.{js,md,json}\"",
    "docs:structure": "node scripts/update-readme-structure.js",
    "toc": "doctoc --title \"## ðŸ“š Table of Contents\" README.md",
    "check": "npm run lint",
    "fix": "npm run format && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ioncakephper/js-starter.git"
  },
  "keywords": [
    "javascript",
    "starter",
    "node"
  ],
  "author": "Your Name <youremail@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ioncakephper/js-starter/issues"
  },
  "homepage": "https://github.com/ioncakephper/js-starter#readme",
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "doctoc": "^2.2.1",
    "eslint-plugin-markdown": "^4.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
